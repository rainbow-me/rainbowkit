{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2019",
    "preserveSymlinks": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "declarationDir": "dist",
    "jsx": "preserve",
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true
  }
}
