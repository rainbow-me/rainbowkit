  "devDependencies": {
    "@vanilla-extract/private": "^1.0.2",
    "@web3-react/core": "^6.1.9",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@ethersproject/transactions": "^5.5.0",
