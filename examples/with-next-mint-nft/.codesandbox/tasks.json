{
  "$schema": "https://codesandbox.io/schemas/tasks.json",
  "setupTasks": [
    {
      "name": "Upgrading RainbowKit to the latest version",
      "command": "yarn up @rainbow-me/rainbowkit"
    },
    {
      "name": "Installing dependencies",
      "command": "yarn install"
    }
  ],
  "tasks": {
    "dev": {
      "name": "Start Dev Server",
      "command": "yarn dev",
      "runAtStart": true
    }
  }
}
